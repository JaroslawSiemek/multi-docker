{
  "AWSEBDockerrunVersion": "1",
  "containerDefinitions": [
    {
      "name": "client",
      "image": "jaroslawsiemek/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "jaroslawsiemek/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "jaroslawsiemek/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "jaroslawsiemek/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "Ports": [
        {
          "ContainerPort": "80"
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
